{
  "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
  ],
  "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
  },
  "eslint.workingDirectories": [
      {
          "mode": "auto"
      }
  ],
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
}