{
  "name": "functions",
  "scripts": {
    "build": "eslint --fix --ext ts,tsx . && tsc -w",
    "serve": "npm run build | firebase emulators:start --import=./emulated_data --export-on-exit",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.18.0"
  },
  "devDependencies": {
    "typescript": "^4.5.4",
    "firebase-functions-test": "^0.2.0"
  },
  "private": true
}
